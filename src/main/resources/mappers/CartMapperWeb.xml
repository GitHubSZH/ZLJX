<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zljx.mapper.CartMapperWeb">

 <select id="selectListLimt" resultType="com.zljx.pojo.Cart">
     select * from cart where cart_type=#{type}
 </select>

 <select id="webPre" resultType="com.zljx.pojo.Cart">
     select * from cart where id &lt;#{id} and cart_type=#{type} GROUP BY id DESC LIMIT 1
 </select>

    <select id="webNext" resultType="com.zljx.pojo.Cart">
     select * from cart where id &gt;#{id} and cart_type=#{type} GROUP BY id ASC LIMIT 1
 </select>

</mapper>